    <!-- Footer accordions -->
    <script defer>
      document.querySelectorAll('.collapsible .collapse-btn').forEach(btn=>{
        btn.addEventListener('click', ()=>{
          const list = document.getElementById(btn.getAttribute('aria-controls'));
          const open = btn.getAttribute('aria-expanded') === 'true';
          btn.setAttribute('aria-expanded', String(!open));
          list.hidden = open;
        });
        const list = document.getElementById(btn.getAttribute('aria-controls'));
        if (window.matchMedia('(max-width:640px)').matches) list.hidden = true;
      });
    </script>

    <!-- Services flyout toggle -->
    <script defer>
      (function () {
        const btn  = document.querySelector('.nav-group-btn[aria-controls="nav-services"]');
        const list = document.getElementById('nav-services');
        if (!btn || !list) return;
        const sync = () => { list.hidden = btn.getAttribute('aria-expanded') !== 'true'; };
        sync();
        btn.addEventListener('click', () => {
          const open = btn.getAttribute('aria-expanded') === 'true';
          btn.setAttribute('aria-expanded', String(!open)); sync();
        });
        document.addEventListener('click', (e) => {
          if (!btn.contains(e.target) && !list.contains(e.target)) {
            btn.setAttribute('aria-expanded', 'false'); sync();
          }
        }, { capture: true });
        document.addEventListener('keydown', (e) => {
          if (e.key === 'Escape') { btn.setAttribute('aria-expanded', 'false'); sync(); }
        });
      })();
    </script>
